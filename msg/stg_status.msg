
uint64 timestamp		# time since system start (microseconds)

uint16 voltage_battery 		# [mV] Напряжение батареи в милливольтах. Рабочие значения в диапазоне 0 … 12600.*/
uint16 voltage_generator 	# [mV] Напряжение генератора в милливольтах. Рабочие значения в диапазоне 0 … 36000.*/
uint16 current_battery 		# [mA] Ток аккумулятора в миллиамперах. Рабочие значения в диапазоне 0 … 30000.*/
uint16 power_load 		# [mW] Мощность потребляемая нагрузкой. Рабочие значения в диапазоне 0 … 300.*/
uint16 current_charge 		# [mA] Ток зарядки аккумулятора. Если 0 – зарядка не идет. Рабочие значения в диапазоне 0 … 2000.*/
int16 temperarture_bridge 	# [C] Температура моста. Рабочие значения в диапазоне -40 … 150.*/
int16 rpm_cranckshaft		# [rpm] Частота оборотов коленвала в минуту. Рабочие значения в диапазоне 0…10000.*/
uint16 uptime 			# [s] Время после включения в мс.*/
int16 engine_temperature 	# [C] Температура двигателя. */
uint8 fuel_level		# [%] Уровень топлива.*/
uint8 motor_state 		# Состояние мотора.*/
uint8 stg_errors_bitmask 	# Битовая маска ошибок.*/
